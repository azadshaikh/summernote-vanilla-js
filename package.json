{
  "name": "summernote",
  "description": "Super simple WYSIWYG editor",
  "version": "0.9.1",
  "license": "MIT",
  "keywords": [
    "editor",
    "WYSIWYG"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/summernote/summernote.git"
  },
  "author": {
    "name": "hackerwins",
    "email": "susukang98@gmail.com"
  },
  "main": "dist/v2/asteronote.js",
  "module": "dist/v2/asteronote.esm.js",
  "exports": {
    ".": {
      "require": "./dist/v2/asteronote.js",
      "import": "./dist/v2/asteronote.esm.js"
    },
    "./lite": {
      "require": "./dist/v2/asteronote-lite.js",
      "import": "./dist/v2/asteronote-lite.esm.js"
    }
  },
  "files": [
    "dist/v2",
    "src/v2"
  ],
  "type": "module",
  "engines": {
    "node": ">=17.0.0"
  },
  "scripts": {
    "dev": "vite --config vite.config.dev.js",
    "prebuild": "node ./scripts/build-fonts.js",
    "build": "vite build",
    "buildall": "npm run prebuild && node ./scripts/build-all.js",
    "lint": "eslint src public/plugin public/lang test scripts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build:v2": "rollup -c rollup.config.js",
    "build:v2:watch": "rollup -c rollup.config.js --watch",
    "build:v2:dev": "rollup -c rollup.config.js --environment BUILD:development",
    "build:v2:prod": "rollup -c rollup.config.js --environment BUILD:production"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-external-helpers": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@popperjs/core": "^2.11.8",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/browser": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-module-resolver": "^5.0.2",
    "bootstrap": "^5.3.8",
    "chromedriver": "^141.0.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.1",
    "eslint": "^9.37.0",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "less": "^4.4.2",
    "less-loader": "^12.3.0",
    "resolve-url-loader": "^5.0.0",
    "rollup": "^4.52.4",
    "rollup-plugin-external-globals": "^0.13.0",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "serve-static": "^2.2.0",
    "uglify-js": "^3.19.3",
    "vite": "^7.1.10"
  }
}
